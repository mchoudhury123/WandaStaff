{
  "name": "WandaStaff",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "reset:setup": "node scripts/setup-reset.js",
    "reset:backup": "node scripts/backup-firestore.js",
    "reset:purge": "node scripts/purge-firestore.js",
    "reset:purge:dry": "node scripts/purge-firestore.js --dry-run",
    "reset:verify": "node scripts/verify-reset.js",
    "reset:seed": "node scripts/seed-data.js --confirm",
    "env:staging": "cp .env.staging .env",
    "env:production": "cp .env.production .env",
    "env:setup": "node scripts/setup-env.js",
    "verify:firebase": "node scripts/verify-firebase.js",
    "setup:eas-secrets": "node scripts/setup-eas-secrets.js",
    "setup:eas-secrets:staging": "node scripts/setup-eas-secrets.js staging",
    "setup:eas-secrets:production": "node scripts/setup-eas-secrets.js production",
    "test:eas-config": "node scripts/test-eas-configuration.js",
    "build:staging:android": "npm run env:staging && npm run build:android",
    "build:staging:ios": "npm run env:staging && npm run build:ios",
    "build:production:android": "npm run env:production && npm run build:android",
    "build:production:ios": "npm run env:production && npm run build:ios",
    "build:eas:staging:android": "eas build --platform android --profile staging",
    "build:eas:staging:ios": "eas build --platform ios --profile staging",
    "build:eas:production:android": "eas build --platform android --profile production",
    "build:eas:production:ios": "eas build --platform ios --profile production",
    "build:eas:all:staging": "eas build --platform all --profile staging",
    "build:eas:all:production": "eas build --platform all --profile production"
  },
  "dependencies": {
    "@expo/webpack-config": "^19.0.1",
    "@react-native-async-storage/async-storage": "^1.18.2",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "expo": "~49.0.0",
    "expo-location": "~16.1.0",
    "expo-notifications": "~0.20.1",
    "firebase": "^9.23.0",
    "leaflet": "^1.9.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "^4.2.1",
    "react-native": "0.72.10",
    "react-native-dotenv": "^3.4.11",
    "react-native-maps": "^1.7.1",
    "react-native-paper": "^5.10.6",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.2",
    "@types/react-native-vector-icons": "^6.4.18",
    "typescript": "^5.1.3"
  },
  "private": true
}
